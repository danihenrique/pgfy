{
  "extends": ["airbnb"],
  "env": {
    "node": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 8
  },
  "rules": {
    "no-console": 0,
    "no-unused-vars": 0,
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "global-require": 0,
    "import/no-dynamic-require": 0,
    "no-await-in-loop": 0,
    "no-restricted-syntax": 0,
    "import/no-unresolved": 0
  }
}
